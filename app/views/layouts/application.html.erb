<!DOCTYPE html>
<html>

<head>
  <title>GCampDavidDang</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<div class ="container">
<body>


<nav class="navbar navbar-default navbar-fixed-top navbar-inverse" role="navigation">
  <div class="container">
    <a class="navbar-brand" href="/home">gCamp</a>
    <ul class="nav navbar-nav navbar-right">



    <% if current_user %>
    <li> <%= link_to "#{current_user.first_name} #{current_user.last_name}", user_path(current_user) %> </li>
    <li class="divider"></li>
    <li><%= link_to "Sign Out", signout_path %></li>
    <% else %>
    <li><%= link_to "Sign Up", signup_path %></li>
    <li class="divider"></li>
    <li><%= link_to "Sign In", signin_path %></li>
    <% end %>
  </ul>
</nav>

<%= yield %>










</body>


<hr>
<div class ="container">

<footer>
  <%= link_to "About", about_path %> | <%= link_to "Terms", terms_path %> | <%= link_to "FAQ", faq_path %> | <%= link_to "Tasks", tasks_path %> | <%= link_to "Users", users_path %> | <%= link_to "Projects", projects_path %>
</footer>

</div>
</html>
